{% extends "page/index.html.twig" %}

{% block main %}
<div class="container">
    <form>
        <div class="form-group">
            <label for="luogo">Luogo</label>
            <input type="text" class="form-control" id="luogo" placeholder="Luogo">
        </div>
        <div class="form-group">
            <label for="squadradicasa">Squadra di casa</label>
            <input type="text" class="form-control" id="squadradicasa" placeholder="Squadra di casa">
        </div>
        <div class="form-group">
            <label for="squadra ospite">Squadra ospite</label>
            <input type="text" class="form-control" id="squadraospite" placeholder="Squadra ospite">
        </div>
        <div class="form-group">
            <label for="arbitro">Arbitro</label>
            <input type="text" class="form-control" id="arbitro" placeholder="Arbitro">
        </div>
        <div class="form-group">
            <label for="dataeora">Data e ora</label>
            <input type="datetime-local" class="form-control" id="dataeora" placeholder="Data e ora">
        </div>
        <button type="submit" class="btn btn-primary">Conferma</button>
    </form>
</div>
{% endblock %}